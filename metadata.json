  "description": "Manage SSH",
  "dependencies": [
    {"name":"puppetlabs/stdlib","version_requirement":">= 5.2.0 < 6.0.0"},
    {"name":"puppetlabs/concat","version_requirement":">= 5.2.0 < 6.0.0"},
    {"name":"puppetlabs/sshkeys_core","version_requirement":">= 1.0.2 < 2.0.0"}
